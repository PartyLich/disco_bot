{
  "name": "disco_bot",
  "version": "0.3.0",
  "description": "Discord music bot featuring Lucio",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "startb": "babel-node index.js",
    "mon": "nodemon --exec babel-node src/index.js",
    "test": "babel-tape-runner **/*.test.js | faucet",
    "clean": "rimraf dist",
    "build": "npm run clean && babel src -d dist --copy-files --ignore \"src/**/*.spec.js\",\"src/**/*.test.js\""
  },
  "keywords": [
    "discord",
    "bot",
    "music",
    "overwatch"
  ],
  "author": "PartyLich",
  "license": "ISC",
  "babel": {
    "presets": [
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "usage"
          }
        ]
      ]
  },
  "dependencies": {
    "bufferutil": "^4.0.1",
    "discord.js": "^11.5.1",
    "ffmpeg": "0.0.4",
    "ffmpeg-static": "^2.4.0",
    "fluent-ffmpeg": "^2.1.2",
    "node-opus": "^0.3.2",
    "ytdl-core": "^0.29.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "babel-tape-runner": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.13.0",
    "nodemon": "^1.19.1",
    "tape": "^4.10.2"
  }
}
